<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
<%= render 'books/list', user: @user, book: @book %>
    </div>

    <div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table table-hover table-inverse">
    <thead>
      <th></th>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </thead>
    <tbody>
      <% @user_all.each do |all| %>
      <tr>
        <td><%=image_tag all.get_profile_image(100,100)%></td>
        <td><%= all.name %></td>
        <td><%= link_to "Show", user_path(all.id) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

</div>
</div>
</div>